{
  "name": "eea-webform",
  "version": "1.0.0",
  "description": "",
  "main": "../*.js",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.3.0",
    "request": "^2.81.0",
    "request-mocha": "^0.2.0",
    "minify-all": "^1.2.2",
    "uglify-js": "^3.0.11",
    "uglifycss": "0.0.27",
    "wcag": "^0.3.0"
  },
  "scripts": {
    "lint": "eslint ../*.js",
    "test": "TEST_URL=https://www.eea.europa.eu/help/contact-info mocha --compilers js:babel-register --timeout 15000 -- ",
    "testc": "TEST_URL=https://www.eea.europa.eu/help/contact-info istanbul cover _mocha -- --compilers js:babel-register --timeout 15000 -- ",
    "buildall": "babel ../assets/components/*.js -d ./dist/components --no-babelrc --presets=es2015 --ignore *.min.js && npm run minjs  && npm run mincom  && npm run mincss",
    "build": "npm run minjs  && npm run mincss",
    "minjs": "babel ../$FNM$.webform.js | uglifyjs -o ./dist/$FNM$.webform.min.js",
    "mincss": "uglifycss ../$FNM$.webform.css --output ./dist/$FNM$.webform.min.css ",
    "mincom": "minify-all ./dist/assets/components",
    "deploy": "babel ../$FNM$.webform.js | uglifyjs -o ../$FNM$.webform.min.js && uglifycss ../$FNM$.webform.css --output ../$FNM$.webform.min.css"
  },
  "author": "EEA aurthor",
  "license": "ISC"
}
