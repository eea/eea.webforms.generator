{
  "name": "eea-webform",
  "version": "1.0.0",
  "description": "",
  "main": "../*.js",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.3.0",
    "request": "^2.81.0",
    "request-mocha": "^0.2.0",
    "uglify-js": "^3.0.11",
    "uglifyjs-folder": "^1.1.0",
    "uglifycss": "0.0.27",
    "wcag": "^0.3.0"
  },
  "scripts": {
    "lint": "eslint ../*.js",
    "test": "mocha --compilers js:babel-register",
    "testc": "istanbul cover _mocha -- --compilers js:babel-register",
    "buildall": "babel ../assets/components/*.js -d ./dist/components --no-babelrc --presets=es2015 --ignore *.min.js && npm run minjs  && npm run mincom  && npm run mincss",
    "build": "npm run minjs  && npm run mincss",
    "minjs": "babel ../$FNM$.webform.js | uglifyjs -o ./dist/$FNM$.webform.min.js",
    "mincss": "uglifycss ../$FNM$.webform.css --output ./dist/$FNM$.webform.min.css ",
    "mincom": "uglifyjs-folder ./dist/assets/components -e -o -y ./dist/assets/components-min",
    "deploy": "babel ../$FNM$.webform.js | uglifyjs -o ../$FNM$.webform.min.js && uglifycss ../$FNM$.webform.css --output ../$FNM$.webform.min.css",
    "deployall": "babel ../assets/components/*.js -d ./dist/components --no-babelrc --presets=es2015,stage-2 --ignore *.min.js && npm run deploy"
  },
  "author": "EEA aurthor",
  "license": "ISC"
}
