{
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
    "angular": 1,
    "document": 1,
    "groups": 1,
    "Version": 1,
    "$": 1,
    "eea": 1,
    "app": 1
  },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console":0,
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}