    "npm": "^4.5.0",
    "openurl": "^1.1.1",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "uglify-js": "^3.0.11",
    "uglifyjs-folder": "^1.1.0",
    "uglifycss": "0.0.27",
    "wcag": "^0.3.0",
    "webshot": "^0.18.0",
    "xmldoc": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "heapdump": "^0.3.9",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.3.0",
    "recursive-uglifyjs": "^1.0.0",
    "request": "^2.81.0",
    "request-mocha": "^0.2.0"
  },
  "scripts": {
    "lint": "eslint src/**/*webform*",
    "test": "mocha --compilers js:babel-register --timeout 5000",
    "testc": "istanbul cover _mocha -- --compilers js:babel-register --timeout 5000",
    "testh": "istanbul cover _mocha -- --compilers js:babel-register -c --timeout 5000",
    "dev": "nodemon ./src/xsdwebform.js --exec babel-node --presets es2015,stage-2",
    "deva": "nodemon ./src/xsdwebform.js -a --exec babel-node --presets es2015,stage-2",
    "build": "babel ./src -d dist --presets es2015,stage-2",
    "buildcom": "babel ./src/client/assets/components/*.js -d /tmp/components --no-babelrc --presets=es2015 --ignore *.min.js && uglifyjs-folder /tmp/components/src/client/assets/components -eo ./src/client/assets/components ",
    "buildser": "babel ./src/client/assets/services/*.js -d /tmp/services --no-babelrc --presets=es2015 --ignore *.min.js && uglifyjs-folder /tmp/services/src/client/assets/services -eo ./src/client/assets/services ",
    "buildeea": "uglifyjs ./src/client/assets/eea/eea.core.js -o ./src/client/assets/eea/eea.core.min.js"
  },
